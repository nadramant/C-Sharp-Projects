using System;
using System.Collections.Generic;
using System.Text;

namespace MemoryGame
{
    public class Cell<T>
    {
        private T m_Contant;
        private bool m_IsRevealed = false;
        private bool m_IsSetted = false;


        public bool IsCellSetted
        {
            get
            {
                return m_IsSetted;
            }
            set
            {
                m_IsSetted = value;
            }
        }

        public bool IsRevealed
        {
            get
            {
                return m_IsRevealed;
            }
            set
            {
                m_IsRevealed = value;
            }
        }

        public T CellContant
        {
            get
            {
                return m_Contant;
            }
            set
            {
                m_Contant = value;
            }
        }
    }
}
